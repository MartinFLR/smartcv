{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "defaultBase": "master",
  "nxCloudId": "69116f3a32ad553ebebc1318",
  "parallel": 5,
  "maxCacheSize": "10GB",
  "neverConnectToCloud": false,
  "namedInputs": {
    "default": ["{projectRoot}/**/*", "{workspaceRoot}/**/*"],
    "production": ["!{projectRoot}/**/*.spec.ts", "!{projectRoot}/**/*.test.ts"]
  },
  "targetDefaults": {
    "build": {
      "inputs": ["default", "{projectRoot}/tsconfig.*.json"]
    },
    "lint": {
      "inputs": ["default", "{workspaceRoot}/.eslintrc.json"]
    },
    "test": {
      "inputs": ["default", "{workspaceRoot}/jest.config.js"]
    }
  }
}
