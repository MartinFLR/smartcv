  "targetDefaults": {
    "build": { "inputs": ["default", "{projectRoot}/tsconfig.*.json"] },
    "lint": { "inputs": ["default", "{workspaceRoot}/.eslintrc.json"] },
    "test": { "inputs": ["default", "{workspaceRoot}/jest.config.js"] }
  },
  "nxCloudId": "691396e57ee27f635d6a2560"
}
